# Set these to 'true' to show the respective OAuth buttons
GoauthOn=true
EouathOn=true
# Example environment variables for VibeNest
# Copy this file to `.env` and fill values (do NOT commit secrets to git)

# App
PORT=3000
NODE_ENV=development
SESSION_SECRET=replace-with-a-secure-random-string
BASE_URL=http://localhost:3000

# Support / legal emails
SUPPORT_EMAIL=kinvilleadam@maltaschools.org
LEGAL_EMAIL=kinvilladam134@outlook.com

## Mailjet (primary — used for all email if set)
MAILJET_API_KEY=
MAILJET_API_SECRET=
MAILJET_SMTP_HOST=in-v3.mailjet.com
MAILJET_SMTP_PORT=587
MAILJET_SMTP_SECURE=false

## SMTP (fallback — used only if Mailjet not set)
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASS=
SMTP_SECURE=false

# Google OAuth (optional)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_CALLBACK_URL=http://localhost:3000/auth/google/callback

# Microsoft / Entra ID OAuth (optional)
ENTRA_CLIENT_ID=
ENTRA_CLIENT_SECRET=
ENTRA_CALLBACK_URL=http://localhost:3000/auth/entra/callback

# Helpful if using Nodemailer with Gmail (not recommended for production)
NODEMAILER_USER=
NODEMAILER_PASS=
